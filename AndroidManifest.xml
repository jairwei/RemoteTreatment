<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.remotetreatment"
	android:versionCode="1"
	android:versionName="1.0">
	<uses-sdk
		android:minSdkVersion="11"
		android:targetSdkVersion="19" />
	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:theme="@style/AppTheme"
		android:name="com.example.remotetreatment.GlobalApplication">
		<activity
			android:name="com.example.remotetreatment.activity.SplashActivity"
			android:theme="@android:style/Theme.NoTitleBar.Fullscreen">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="com.example.remotetreatment.activity.LoginActivity"
			android:theme="@android:style/Theme.NoTitleBar" />
		<activity
			android:name="com.example.remotetreatment.activity.RegisterActivity"
			android:theme="@android:style/Theme.NoTitleBar" />
		<activity
			android:name="com.example.remotetreatment.activity.HomeActivity"
			android:theme="@android:style/Theme.NoTitleBar" />
		<activity
			android:name="com.example.remotetreatment.activity.DoctorDetailActivity"
			android:theme="@android:style/Theme.NoTitleBar" />
		<activity
			android:name="com.example.remotetreatment.activity.DoctorReserveActivity"
			android:theme="@android:style/Theme.NoTitleBar" />
	</application>
</manifest>